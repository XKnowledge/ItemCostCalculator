{
  "name": "ItemCostCalculator",
  "version": "1.0.0",
  "description": "物品成本计算",
  "main": "main.js",
  "author": "XKnowledge Team",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.ItemCostCalculator.ItemCostCalculator",
    "productName": "ItemCostCalculator",
    "compression": "store",
    "directories": {
      "output": "release"
    },
    "asar": true,
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "!**/node_modules/**/*"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "4.x",
    "axios": "^1.6.8",
    "echarts": "^5.5.0",
    "electron-updater": "^6.1.7",
    "fs": "^0.0.1-security",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "vite": "^5.0.12",
    "vue": "^3.4.15"
  }
}
